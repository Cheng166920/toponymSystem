<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" "http://mybatis.org/dtd/mybatis-3-mapper.dtd" >
<mapper namespace="com.example.toponym.mapper.BoundaryAdminMapper">
    <select id="findAll" resultType="com.example.toponym.model.BoundaryAdmin">
        SELECT *
        FROM public."B_A_T_SUBJECT_A"
    </select>
    <select id="findByAdmin"  parameterType="String" resultType="java.util.Map">
        SELECT *
        FROM public."B_A_T_SUBJECT_A"
        WHERE "ADMIN"=#{admin}
    </select>
</mapper>